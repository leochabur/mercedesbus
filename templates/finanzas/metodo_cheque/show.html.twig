{% block title %}MetodoCheque{% endblock %}

{% block body %}
    <h1>MetodoCheque</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ metodo_cheque.id }}</td>
            </tr>
            <tr>
                <th>Importe</th>
                <td>{{ metodo_cheque.importe }}</td>
            </tr>
            <tr>
                <th>NumeroCheque</th>
                <td>{{ metodo_cheque.numeroCheque }}</td>
            </tr>
