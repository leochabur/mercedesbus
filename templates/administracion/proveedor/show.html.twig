{% extends 'base.html.twig' %}

{% block title %}Proveedor{% endblock %}

{% block body %}
    <h1>Proveedor</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ proveedor.id }}</td>
            </tr>
            <tr>
                <th>RazonSocial</th>
                <td>{{ proveedor.razonSocial }}</td>
            </tr>
            <tr>
                <th>Cuit</th>
                <td>{{ proveedor.cuit }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ proveedor.activo ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ proveedor.direccion }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ proveedor.telefono }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ proveedor.mail }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_administracion_proveedor_index') }}">back to list</a>

    <a href="{{ path('app_administracion_proveedor_edit', {'id': proveedor.id}) }}">edit</a>

    {{ include('administracion/proveedor/_delete_form.html.twig') }}
{% endblock %}
